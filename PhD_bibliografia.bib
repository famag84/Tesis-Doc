month = {dec},
number = {6},
pages = {3227--33},
pmid = {10585944},
publisher = {Elsevier},
title = {{Microsecond time-scale discrimination among polycytidylic acid, polyadenylic acid, and polyuridylic acid as homopolymers or as segments within single RNA molecules.}},
