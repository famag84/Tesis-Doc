journal = {Nature nanotechnology},
keywords = {Biosensing Techniques,DNA,DNA: chemistry,DNA: genetics,DNA: instrumentation,DNA: methods,Nanopores,Nanowires,Nanowires: chemistry,Sequence Analysis,Silicon},
month = {feb},
number = {2},
pages = {119--25},
pmid = {22157724},
publisher = {Nature Publishing Group},
title = {{Local electrical potential detection of DNA by nanowire-nanopore sensors}},
