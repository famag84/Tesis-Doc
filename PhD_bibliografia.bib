keywords = {Acrylamides,Acrylamides: chemistry,Ions,Ions: chemistry,Nanostructures,Nanostructures: chemistry,Phase Transition,Polymers,Polymers: chemistry,Temperature},
month = {mar},
number = {4},
pages = {859--64},
pmid = {20140936},
title = {{Current rectification in temperature-responsive single nanopores.}},
