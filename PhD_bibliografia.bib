issn = {1520-5126},
journal = {Journal of the American Chemical Society},
keywords = {Electrolytes,Electrolytes: chemistry,Hydrogen-Ion Concentration,Ions,Ions: chemistry,Kinetics,Nanostructures,Nanostructures: chemistry,Nanotechnology,Nanotechnology: methods,Polymers,Polymers: chemistry},
month = {feb},
