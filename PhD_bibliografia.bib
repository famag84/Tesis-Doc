@article{OHern2014,
abstract = {We report selective ionic transport through controlled, high-density, subnanometer diameter pores in macroscopic single-layer graphene membranes. Isolated, reactive defects were first introduced into the graphene lattice through ion bombardment and subsequently enlarged by oxidative etching into permeable pores with diameters of 0.40 ± 0.24 nm and densities exceeding 10(12) cm(-2), while retaining structural integrity of the graphene. Transport measurements across ion-irradiated graphene membranes subjected to in situ etching revealed that the created pores were cation-selective at short oxidation times, consistent with electrostatic repulsion from negatively charged functional groups terminating the pore edges. At longer oxidation times, the pores allowed transport of salt but prevented the transport of a larger organic molecule, indicative of steric size exclusion. The ability to tune the selectivity of graphene through controlled generation of subnanometer pores addresses a significant challenge in the development of advanced nanoporous graphene membranes for nanofiltration, desalination, gas separation, and other applications.},
author = {O'Hern, Sean C and Boutilier, Michael S H and Idrobo, Juan-Carlos and Song, Yi and Kong, Jing and Laoui, Tahar and Atieh, Muataz and Karnik, Rohit},
doi = {10.1021/nl404118f},
file = {:home/facundo/Documents/Dropbox/mendeley/2014{\_}O'Hern et al.{\_}Nano letters.pdf:pdf},
issn = {1530-6992},
journal = {Nano letters},
keywords = {a new class of,allotrope of carbon,an sp 2 -bonded,be the backbone for,desalination,fi lter,highly permeable,highly selective molecular sieve,ion selective membrane,materials for both liquid-,molecular sieve,nano fi ltration,promises to,raphene},
month = {mar},
number = {3},
pages = {1234--41},
pmid = {24490698},
title = {{Selective ionic transport through tunable subnanometer pores in single-layer graphene membranes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24490698},
volume = {14},
year = {2014}
}
