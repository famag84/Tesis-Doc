@article {Ke1177_DNA_selfassembly_2012,
    author = {Ke, Yonggang and Ong, Luvena L. and Shih, William M. and Yin, Peng},
    title = {Three-Dimensional Structures Self-Assembled from DNA Bricks},
    volume = {338},
    number = {6111},
    pages = {1177--1183},
    year = {2012},
    doi = {10.1126/science.1227268},
    publisher = {American Association for the Advancement of Science},
    abstract = {We describe a simple and robust method to construct complex three-dimensional (3D) structures by using short synthetic DNA strands that we call {\textquotedblleft}DNA bricks.{\textquotedblright} In one-step annealing reactions, bricks with hundreds of distinct sequences self-assemble into prescribed 3D shapes. Each 32-nucleotide brick is a modular component; it binds to four local neighbors and can be removed or added independently. Each 8{\textendash}base pair interaction between bricks defines a voxel with dimensions of 2.5 by 2.5 by 2.7 nanometers, and a master brick collection defines a {\textquotedblleft}molecular canvas{\textquotedblright} with dimensions of 10 by 10 by 10 voxels. By selecting subsets of bricks from this canvas, we constructed a panel of 102 distinct shapes exhibiting sophisticated surface features, as well as intricate interior cavities and tunnels.},
    issn = {0036-8075},
    URL = {http://science.sciencemag.org/content/338/6111/1177},
    eprint = {http://science.sciencemag.org/content/338/6111/1177.full.pdf},
    journal = {Science}
}

